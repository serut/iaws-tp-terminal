<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:hr="http://fr/ups/dl/iaws/contractfirst/results"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           targetNamespace="http://fr/ups/dl/iaws/contractfirst/results">
    <xs:element name="CinemaRequest">
        <xs:complexType>
            <xs:all>
                <xs:element name="Film" type="hr:FilmType"/>
            </xs:all>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="FilmType">
        <xs:sequence>
            <xs:element name="id" type="xs:int"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>